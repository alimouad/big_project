<div class="container">
     <div class="loader">
         <div class="sk-cube-grid">
             <div class="sk-cube sk-cube1"></div>
             <div class="sk-cube sk-cube2"></div>
             <div class="sk-cube sk-cube3"></div>
             <div class="sk-cube sk-cube4"></div>
             <div class="sk-cube sk-cube5"></div>
             <div class="sk-cube sk-cube6"></div>
             <div class="sk-cube sk-cube7"></div>
             <div class="sk-cube sk-cube8"></div>
             <div class="sk-cube sk-cube9"></div>
         </div>
     </div>
<%- include('./partials/side.ejs')  %>
<div class="main">
   

<%- include('./partials/header.ejs')  %>
<div id="map" class="map">
 <div id="query_tab">
     <div class="tab-content" id="nav-tabContent">
         <div class="tab-pane fade show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">
             <label for="layer"><b>Select Layer</b></label>
             <select id="layer" class="form-select form-select-sm" aria-label=".form-select-sm example">
                 <option selected>Select Layer</option>
             </select>
             <br>
             <label for="attributes"><b>Select Attribute</b></label>
             <select id="attributes" class="form-select form-select-sm" aria-label=".form-select-sm example">
                 <option selected>Select Attribute</option>
             </select>
             <br>
             <label for="operator"><b>Select Attribute</b></label>
             <select id="operator" class="form-select form-select-sm" aria-label=".form-select-sm example">
                 <option selected>Select Operator</option>
             </select>
             <br>
             <label for="value">Enter Value</label>
             <input type="text" class="form-control form-select-sm" id="value" name="value">
             <br>
             <div class="flex">
                <button onclick="query()" type="button" class="btn btn-danger btn-sm">Query</button>
             <button onclick="removeQuery()" type="button" class="btn btn-danger btn-sm"> Rv query</button>
             </div>
             

         </div>
     </div>
 </div>

 <div class="tabs">
     <a href="#" class="close-btn"><i class="fa-solid fa-xmark"></i></a>
     <div class="tab-header">
         <div class="active first">Buffer </div>
         <div>Intersect </div>
         <div>Union </div>
     </div>
     <div class="tab-indicator"></div>
     <div class="tab-body">
         <div class="active first buffer" >
             <form action="#" onsubmit="return false;">
                <div class="flex row">
                   <div>
                       <input type="checkbox" id="s1"  class="switch" name="s1" checked>
                       <label for="s1"></label>   
                   </div>
                   <span class="download_file btn"><i class="ph ph-file-arrow-down"></i></span>
                </div>
                <p class="descr">Calculates a buffer for input features for a given radius. Units supported are miles,
                    kilometers, and degrees.</p>
                 
                 <div class="inputs">
                    <div>
                        <label for="tool-layer"><b>Select Layer</b></label>
                        <select id="tool-layer" class="form-select form-select-sm" aria-label=".form-select-sm example">
                            <option selected>Select Layer</option>
                        </select>
                    </div>
                    
                     <!-- <input type="text" name="" id="xCar" class="coordinate" placeholder=" "> -->
                     <input type="text" name="" id="yCar" class="buffer-distance-input coordinate"
                         placeholder="buffer distance (meters)">
                 </div>
                 <div class="flex">
                    <button id="buffer-tool" onclick="applyBuffer()" class="btn">Apply Buffer</button>
                     
                 </div>
                 
             </form>
         </div>
         <div class="intersection">
             <form action="#" onsubmit="return false;">
                 <div class="flex row">
                     <div>
                         <input id="s2" type="checkbox" class="switch" name="s2" checked>
                         <label for="s2"></label>
                         <!-- <input type="checkbox" id="switch" /><label class="swicth" for="switch" >Toggle</label> -->
                     </div>
                     <span class="download_file btn"><i class="ph ph-file-arrow-down"></i></span>
                 </div>
                 <p class="desc">Takes polygon or multi-polygon geometries and finds their polygonal intersection. If
                     they don't intersect, returns null.</p>

                 <div class="inputs">
                    
                     <div>
                         <label for="tool-layer-tree"><b>Select First Layer</b></label>
                         <select id="tool-layer-tree" class="form-select form-select-sm"
                             aria-label=".form-select-sm example">
                             <option selected>Select First Layer</option>
                         </select>
                     </div>

                     <!-- <input type="text" name="" id="xCar" class="coordinate" placeholder=" "> -->
                    <div>
                        <label for="tool-layer-four"><b>Select Second Layer</b></label>
                        <select id="tool-layer-four" class="form-select form-select-sm" aria-label=".form-select-sm example">
                            <option selected>Select Second Layer</option>
                        </select>
                    </div>
                 </div>
                 <div class="flex">
                       <button id="intersectBtn" class="btn" onclick="applyIntersection()">Apply Intersect</button>
               
                 </div>
              
             </form>
         </div>
         <div class="union">
             <form action="#" onsubmit="return false;">
               <div class="flex row">
                   <div>
                    <input type="checkbox" class="switch" id="s3" name="s3" checked>
                    <label for="s3"></label>
                       <!-- <input type="checkbox" id="switch" /><label class="swicth" for="switch" >Toggle</label> -->
                   </div>
                   <span class="download_file btn"><i class="ph ph-file-arrow-down"></i></span>
               </div>
                <p class="descr">Takes input (Multi)Polygon(s) and returns a combined polygon. If the input polygons are
                    not contiguous, this function returns a MultiPolygon feature.</p>

                <div class="inputs">
                    <div>
                        <label for="tool-layer-one"><b>Select First Layer</b></label>
                        <select id="tool-layer-one">
                            <option selected>Select First Layer</option>
                        </select>
                    </div>

                    
                    <div>
                        <label for="tool-layer-two"><b>Select Second Layer</b></label>
                        <select id="tool-layer-two" class="form-select form-select-sm" aria-label=".form-select-sm example">
                            <option selected>Select Second Layer</option>
                        </select>
                    </div>
                </div>
                 <div class="flex">
                    <button id="uniontBtn" class="btn" onclick="applyUnion()">Apply Union</button>

                   
                 </div>
                 
             </form>
         </div>
     </div>
 </div>
</div>
<%- include('./partials/data_restore.ejs')  %>

</div>

</div>
</div>